{
  "applicationId": "template",
  "version": "0.0.1",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/app/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": ["app.*", "window.*", "os.*"],
  "modes": {
    "window": {
      "title": "template",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/app/build/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "nativeBlockList": ["filesystem.*"]
    }
  },
  "cli": {
    "binaryName": "template",
    "resourcesPath": "/app/build/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/app/public/js/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0",
    "frontendLibrary": {
      "patchFile": "/app/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
